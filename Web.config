<?xml version="1.0"?>
<configuration>
    <connectionStrings>
        <add name="MoviesDB" connectionString="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\MoviesDB.mdf;Integrated Security=True;MultipleActiveResultSets=True" providerName="System.Data.SqlClient" />
    </connectionStrings>
  
    <system.web>
        <compilation debug="true" targetFramework="4.8" />
    
        <httpRuntime targetFramework="4.8" />
    
        <pages controlRenderingCompatibilityVersion="4.0" clientIDMode="AutoID" />
    
        <customErrors mode="Off" />
    
        <authentication mode="None" />
    </system.web>
  
    <system.webServer>
        <defaultDocument>
   <files>
  <add value="Default.aspx" />
     </files>
   </defaultDocument>
    </system.webServer>
</configuration>













